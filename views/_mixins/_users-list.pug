mixin users-list-form(superheroes, currentPage, pages)
    //- link(rel="stylesheet" type="text/css" href="./static/styles/admin-mixincss")
    div.container
        div.row
            div.col-lg-12
                div.main-box.no-header.clearfix
                    div.main-box-body.clearfix
                        div.table-responsive
                            table.table.user-list
                                thead
                                    tr
                                    th <span>User</span>
                                    th <span> Created</span>
                                    th.text-center <span> Status</span>
                                    th <span> Email</span>
                                    th &nbsp;
                                tbody
                                each user in model
                                    tr
                                        td
                                            img(src=`${user.picture}` alt="")
                                            a(href=`/user/${user.username}` class="user-link")= user.firstName +' '+ user.lastName
                                            span.user-subhead= user.role
                                        td=user.userRating
                                        td.text-center
                                            span.label.label-default Тук може да се сложи
                                        td
                                            a(href="#")=user.email
                                        td(style="width: 20%;")
                                            a.table-link(href="#")
                                                span.fa-stack
                                                    i.fa.fa-square.fa-stack-2x
                                                    i.fa.fa-square.fa-stack-1x.fa-inverse
                                            a.table-link(href="#")
                                                span.fa-stack
                                                    i.fa.fa-square.fa-stack-2x
                                                    i.fa.fa-pencil.fa-stack-1x.fa-inverse
                                            a.table-link danger(href="#")
                                                span.fa-stack
                                                    i.fa.fa-square.fa-stack-2x
                                                    i.fa.fa-trash-o.fa-stack-1x.fa-inverse